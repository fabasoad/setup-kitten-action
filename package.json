{
  "name": "setup-kitten-action",
  "version": "1.0.3",
  "description": "This GitHub action installs Kitten",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-kitten-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint": "eslint --ext ts src",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "test": "jest --config=jest.config.json --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "keywords": [
    "actions",
    "programming-language"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-kitten-action#readme",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.6.1",
    "command-exists": "1.2.9",
    "glob": "7.1.6",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/command-exists": "1.2.0",
    "@types/glob": "7.1.3",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@types/sinon": "9.0.9",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "@vercel/ncc": "0.26.1",
    "chai": "4.2.0",
    "eslint": "7.16.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "mocha-param": "2.0.1",
    "sinon": "9.2.2",
    "snyk": "1.437.2",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is --not main && yarn run build && git add dist/index.js",
      "pre-push": "yarn run lint && yarn test"
    }
  }
}
